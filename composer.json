{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "captcha-com/laravel-captcha": "4.*",
        "consoletvs/charts": "^4.3",
        "doctrine/dbal": "^2.5",
        "dompdf/dompdf": "dev-master",
        "fideloper/proxy": "~4.0",
        "intervention/image": "^2.3",
        "laravel/framework": "5.5.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "maatwebsite/excel": "~2.1.0",
        "meyfa/php-svg": "^0.7.0",
        "nao-pon/flysystem-google-drive": "^1.1",
        "prettus/l5-repository": "^2.6",
        "unisharp/laravel-ckeditor": "^4.6",
        "unisharp/laravel-filemanager": "^1.7"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^2.3",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "filp/whoops": "~2.0"
    },
    "autoload": {
        "files": [
            "Constants/status.php",
            "Constants/db.php"
        ],
        "classmap": [
            "database",
            "modules"
        ],
        "psr-4": {
            "ACL\\": "ACL/",
            "App\\": "app/",
            "Bugger\\": "modules/Bugger/",
            "Edubeanz\\": "modules/Edubeanz/",
            "Games\\": "modules/games/",
            "Istruct\\": "modules/i-struct/",
            "Test\\Rg\\": "modules/test/rg/",
            "Ush\\": "modules/ush/",
            "Docs\\": "modules/Docs/",
            "SuViewer\\": "modules/suviewer/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
